    },
    {
      "name": "textContent",
      "title": "文本内容",
      "type": "String"
    },
    {
      "name": "timestamp",
      "title": "发送时间",
      "type": "DateTime",
      "format": "timestamp-ms"
    },
    {
      "name": "isRead",
      "title": "是否已读",
      "type": "Boolean"
    },
    {
      "name": "emotion",
      "title": "情绪分析",
      "type": "String"
