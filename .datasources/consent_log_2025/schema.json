
{
  "name": "consent_log",
  "title": "用户授权记录2025扩展版",
  "schemas": [
    {
      "name": "consentId",
      "title": "授权ID",
      "type": "String"
    },
    {
      "name": "userId",
      "title": "用户ID",
      "type": "String"
    },
    {
      "name": "consentType",
      "title": "授权类型",
      "type": "String"
    },
    {
      "name": "scope",
      "title": "授权范围",
      "type": "Object"
    },
    {
      "name": "ip",
      "title": "IP地址",
      "type": "String"
    },
    {
      "name": "userAgent",
      "title": "用户代理",
      "type": "String"
    },
    {
      "name": "withdrawable",
      "title": "是否可撤回",
      "type": "Boolean"
    },
    {
      "name": "withdrawn",
      "title": "是否已撤回",
      "type": "Boolean"
    },
    {
      "name": "withdrawnAt",
      "title": "撤回时间",
      "type": "DateTime",
      "format": "timestamp-ms"
    },
    {
      "name": "consentText",
      "title": "授权文本",
      "type": "String"
    },
    {
      "name": "regulation",
      "title": "适用法规",
      "type": "String"
    },
    {
      "name": "regulationVersion",
      "title": "法规版本",
      "type": "String"
    },
    {
      "name": "consentVersion",
      "title": "授权版本",
      "type": "String"
    },
    {
      "name": "validityPeriod",
      "title": "有效期",
      "type": "Object"
    },
    {
      "name": "consentLanguage",
      "title": "授权语言",
      "type": "String"
    },
    {
      "name": "consentMethod",
      "title": "授权方式",
      "type": "String"
    },
    {
      "name": "consentEvidence",
      "title": "授权证据",
      "type": "Object"
    },
    {
      "name": "jurisdiction",
      "title": "管辖区域",
      "type": "String"
    },
    {
      "name": "dataRetention",
      "title": "数据保留",
      "type": "Object"
    }
  ]
}
